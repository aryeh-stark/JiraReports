{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/jira-.log",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ]
  },
  "Environment": "Test",
  "Jira": {
    "BaseUrl": "",
    "ApiToken": "",
    "UserEmail": "",
    "MaxResults": "50",
    "JqlChunkSize": "50",
    "CustomFieldMapping": {
      "Mapping": {
        "Team": "customfield_10001",
        "StoryPoints": "customfield_10104",
        "RelatedSprints": "customfield_10020"
      }
    }
  }
}